{
  "prettier.enable": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "files.associations": {
    "*.css": "postcss"
  },
  "editor.formatOnSave": false,
  "files.exclude": {
    "**/.nuxt": true,
    "**/.output": true
  },
  "i18n-ally.localesPaths": [
    "i18n/locales"
  ],
  "i18n-ally.keystyle": "nested"
}
